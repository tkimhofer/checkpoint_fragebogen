{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "network",
  "description": "Allow HTTP requests to the local/LAN API",
  "windows": ["main"],
  "permissions": [
    {
      "identifier": "http:default",
      "allow": [
        { "url": "http://192.168.10.*/*" },
        { "url": "http://127.0.0.1/*" },
        { "url": "http://localhost/*" },
        { "url": "http://192.168.10.195:9000/*" },
        { "url": "http://192.168.10.195/*" }
      ]
    }
  ]
}
